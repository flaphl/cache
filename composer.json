{
    "name": "flaphl/cache",
    "description": "Caching abstractions pulled from Flaphl elements.",
    "type": "library",
    "license": "MIT",
    "keywords": ["cache", "caching", "flaphl", "php"],
    "authors": [
        {
            "name": "Jade Phyressi",
            "email": "jade@flaphl.com"
        }
    ],
    "provide": {
        "psr/cache-implementation": "1.0|2.0|3.0",
        "psr/simple-cache-implementation": "1.0|2.0",
        "flaphl/cache-implementation": "1.0"
    },
    "require": {
        "php": "^8.2",
        "psr/cache": "^1.0 || ^2.0 || ^3.0",
        "psr/log": "^1.0 || ^2.0 || ^3.0",
        "flaphl/deprecation-contracts": "^2.1",
        "flaphl/cache-contracts": "^1.0"
    },
    "require-dev": {
        "psr/simple-cache": "^1.0 || ^3.0",
        "phpunit/phpunit": "^10.0 || ^11.0"
    },
    "conflict": {
        "flaphl/injection": "<1.0"
    },
    "autoload": {
        "psr-4": { "Flaphl\\Element\\Cache\\": "" },
        "exclude-from-classmap": [""]
    },
    "minimum-stability": "dev",
    "prefer-stable": false
}